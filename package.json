{
  "name": "trends",
  "license": "MIT",
  "devDependencies": {
    "husky": "^1.2.1",
    "prettier": "^1.15.3",
    "pretty-quick": "^1.8.0"
  },
  "scripts": {
    "dev": "pnpm dev -F www -F api",
    "format": "prettier --write \"{api,www}/**/*.{js,jsx,ts,tsx,json,css,md,html,graphql}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
